<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activities.mine.HistoryFeedbackActivity">
    <include
        layout="@layout/titlebar_layout"/>
    <com.yuejian.meet.widgets.springview.SpringView
        android:id="@+id/feedback_spring"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/feedback_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/px_24"
            android:paddingRight="@dimen/px_24"
            android:dividerHeight="@dimen/px_1"
            android:divider="@color/txt_color_999999"
            android:background="@color/white"/>
    </com.yuejian.meet.widgets.springview.SpringView>


</LinearLayout>