<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/contact_all_layout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/setting_bg"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <TextView
        android:id="@+id/contact_letter"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/px_44"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/px_24"
        android:paddingRight="@dimen/px_24"
        android:textColor="@color/color_black_333333"
        android:textSize="@dimen/px_24"
        android:visibility="visible" />

    <View
        android:id="@+id/itemt_wire"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/px_1"
        android:background="@color/vive_colors_wire_e6e6e6"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/px_88"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/px_24"
        android:paddingRight="@dimen/px_24">



        <TextView
            android:id="@+id/text_member_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px_30"
            android:textColor="@color/color_232323"
            android:textSize="@dimen/px_28"
            android:text="姓名"/>
        <TextView
            android:id="@+id/text_member_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/px_30"
            android:textColor="@color/txt_color_999999"
            android:textSize="@dimen/px_28"
            android:text="10086"/>

        <CheckBox
            android:id="@+id/member_check_box"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/selector_choose_sex"
            android:button="@null" />
    </LinearLayout>
</LinearLayout>