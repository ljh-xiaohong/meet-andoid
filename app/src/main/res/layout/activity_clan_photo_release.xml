<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/setting_bg"
    android:orientation="vertical">

    <include layout="@layout/titlebar_layout" />

    <GridView
        android:id="@+id/clan_photo_release_gridveiw"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:horizontalSpacing="@dimen/px_32"
        android:numColumns="4"
        android:paddingBottom="@dimen/px_24"
        android:paddingLeft="@dimen/px_24"
        android:paddingRight="@dimen/px_24"
        android:paddingTop="@dimen/px_24"
        android:verticalSpacing="@dimen/px_16"
        android:visibility="visible" />
</LinearLayout>